{% block paginator %}
<div class=" ajaxBut paginator">
    {% set arrowLeft %} {% endset %}
    {% set arrowRight %} {% endset %}
    {% set arrowDubleRight %} {% endset %}
    {% set arrowDubleLeft %} {% endset %}


     {% if pagin.controller.status is defined and pagin.controller.status == 'arrow' %}
         {% set arrowLeft = "fa fa-"~pagin.controller.name~"-left" %}
         {% set arrowRight = "fa fa-"~pagin.controller.name~"-right"  %}
     {% elseif pagin.controller.status is defined and pagin.controller.status == 'string' %}
         {% set arrowLeft = "Previus" %}
         {% set arrowRight = "Next" %}
     {% elseif pagin.controller.status is defined and pagin.controller.status == 'arrowString' %}
         {% set arrowLeft = "fa fa-"~pagin.controller.name~"-left"  %}
         {% set arrowRight = "fa fa-"~pagin.controller.name~"-right"  %}
     {% endif %}

    {% if pagin.firstLast.status is defined and pagin.firstLast.status == 'arrow'  or pagin.firstLast.status == 'arrowString'%}
        {% set arrowDubleLeft = "fa fa-"~pagin.firstLast.name~"-left" %}
        {% set arrowDubleRight = "fa fa-"~pagin.firstLast.name~"-right"  %}
    {% elseif pagin.firstLast.status is defined and pagin.firstLast.status == 'string' %}
        {% set arrowDubleLeft = "First" %}
        {% set arrowDubleRight = "Last" %}
    {% elseif pagin.firstLast.status is defined and pagin.firstLast.status == 'arrowString'%}
        {% set arrowDubleLeft = "fa fa-"~pagin.firstLast.name~"-left" %}
        {% set arrowDubleRight = "fa fa-"~pagin.firstLast.name~"-right"  %}
    {% endif %}


    {% if  pagin.currentOfTotal is defined and pagin.currentOfTotal %}
        <span class=" ajaxBut active paginate currentOfTotal"> {{ pagin.currentPage }} {{ pagin.trans.of  }} {{ pagin.numPages }} </span>
    {% endif %}

    {% if pagin.firstLast.status is defined %}
        <p class=" ajaxBut {{ pagin.firstLast.firstClass }} paginate" instance="{{ pagin.url }}" 0href="{{ pagin.url }}?url={{ pagin.url }}&page=1&ipp={{ pagin.ipp }}">
            <i class=" ajaxBut  {{ arrowDubleLeft }} aria-hidden=true">
                {% if pagin.firstLast.status != 'arrow' %}
                    {{ pagin.trans.first }}
                {% endif %}
            </i>
        </p>
    {% endif %}

    {% if pagin.prev.prevClass is defined %}
        <p class=" ajaxBut {{ pagin.prev.prevClass }} paginate" instance="{{ pagin.url }}" href="{{ pagin.url }}?url={{ pagin.url }}&page={{ pagin.currentPage -1 }}&ipp="{{ pagin.ipp }}">
            <i class=" ajaxBut {{ arrowLeft }} aria-hidden=true"></i>
            {% if pagin.controller.status != 'arrow' %}
                {{ pagin.trans.previus }}
            {% endif %}
        </p>
    {% else %}
        <span class=" ajaxBut inactive" href="#">{{ pagin.trans.previus }} </span>
    {% endif %}

    {% for key, value in pagin.pages %}
        {% if value.dots is defined and value.dots %}
            <span> ... </span>
        {% else %}
            <p class=" ajaxBut {{ value.class }}" instance="{{ pagin.url }}" title="{{ pagin.trans.goTo }} {{ value.i }} {{ pagin.trans.of }} {{  pagin.numPages }}" href="{{ pagin.url }}?url={{ pagin.url }}&page={{ value.i }}&ipp={{ pagin.ipp }}">{{ value.i }} </p>
        {% endif %}
    {% endfor %}

    {% if  pagin.next.nextClass is defined and pagin.next.nextClass == 'active' %}
        <p class=" ajaxBut {{ pagin.next.nextClass }} paginate" instance="{{ pagin.url }}" href="{{ pagin.url }}?url={{ pagin.url }}&page={{ pagin.currentPage+1 }}&ipp={{ pagin.ipp }}">
            {% if pagin.controller.status != 'arrow' %}
                {{ pagin.trans.next }}
            {% endif %}
            <i class=" ajaxBut {{ arrowRight }} aria-hidden=true"></i>
        </p>
    {% else %}
        <span class=" ajaxBut inactive" title="The last page is" href="#">{{ pagin.trans.next }} </span>
    {% endif %}

    {% if pagin.firstLast.status is defined %}
        <p class=" ajaxBut {{ pagin.firstLast.lastClass }} paginate" instance="{{ pagin.url }}" href="{{ pagin.url }}?url={{ pagin.url }}&page={{ pagin.numPages }}&ipp={{ pagin.ipp }}" >
            {% if pagin.firstLast.status != 'arrow' %}
                {{ pagin.trans.last }}
            {% endif %}
            <i class=" ajaxBut {{ arrowDubleRight }} aria-hidden=true"></i>
        </p>
    {% endif %}

    {% if pagin.all.status is defined and pagin.all.status %}
        <p class=" ajaxBut active paginate" instance="{{ pagin.url }}"  href="{{ pagin.url }}?url={{ pagin.url }}&page=1&ipp=All">{{ pagin.trans.all }}</p>
     {% endif %}

    {% if pagin.ippArray is defined %}
        <select>
            {% for items in pagin.ippArray %}
                <option value={{ items }}>{{ items }}</option>
            {% endfor %}
        </select>
        <label>{{ pagin.trans.perPage }}</label>
    {% endif %}


    {% if pagin.goto is defined %}
        <input type="number" class=" ajaxBut inputPage" min="1" max={{ pagin.numPages }}>
        <input type="hidden" value={{ pagin.url }}>
        <input type="hidden" class=" ajaxBut ipp" value={{ pagin.ipp}}>
        <span class=" ajaxBut paginateToGo paginate">{{ pagin.trans.go }}</span>
    {% endif %}
</div>
{% endblock %}